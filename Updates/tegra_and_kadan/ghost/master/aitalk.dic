//Keeps track of if an hour has passed. Just ignore this.
HourCheck
{
if passmin >= 59
	{
		passmin = 0
		passhour = passhour + 1
		positive += 1
		trust += 1
	}
}

OnMinuteChange //This is what will trigger time-based dialogue for your Ghost.
 {
	HourCheck 
	--
 	if minute == 0 
	{
		//Clearing hourly cooldown variables
		tgivenfood = 0
		kgivenfood = 0
		tgivendrink = 0
		kgivendrink = 0
		mirrortgivenfood = 0
		mirrortgivendrink = 0
 		if hour == 12 && nowshell != "mirror"
		{
 			"\0\s[0]It's noon exactly!\w8\1\n\n[half]\s[10]\i[252]Is that significant in some way?\w8\0\n\n[half]\s[0]\i[406]\i[403]No, I just think it's neat.\i[160]\e"
		}
 		elseif hour == 0 && nowshell != "mirror"
		{
 			"\0\s[0]\i[203]It's exactly midnight!\1\n\n[half]\s[10]\w8You have an oddly precise sense of time.\e"
 			"\0\s[0]Yay! Midnight!\i[158]\w8\1\n\n[half]\s[10]\i[201]Well past time we should all be asleep.\0\s[0]\i[203]\i[251]\w8\n\n[half]Booooo!\i[155]\e"
 			"\1\s[10]\i[201]\i[410]\i[403]Why am I awake at midnight? \w4...I thought I knew what tired was...\i[152]\e"
		}
		else
		{
			"\0\s[0]It's %(hour12) o'clock!\e"
		}
	}
	
	elseif hour == 23 && minute == 55 && nowshell != "mirror"
	{
		"\0\s[0]\i[201]It's five minutes to midnight!\i[158]\e"
	}	
	//Don't touch this part.
	elseif mikireflag == 1
 	{
 		MikireTalk
	}
 }

//--OnSecondChange
//Like the above, this handles what happens every time a second goes by. Most of this you will not have to touch.
OnSecondChange
 {
	if passsec == 59
 		{
	 	passsec = 0
 		passmin = passmin + 1
		}
 	else
 		{
	 	passsec = passsec + 1
		}
	if mikireflag == 0 && reference1 == 1 
	{
		"\0\s[0]Oh! I can't see %(username) for some reason???"
		
		MikireTalk
	}
	if mikireflag == 1 && reference1 == 0 //Dialogue for coming back on the screen
	{
		EndMikireTalk
	}
	mikireflag = reference1 //reference1 is if the ghost is offscreen or not
	if reference2 == 1 && nowshell != "mirror"
	{		
 		"\0\s[0]Haha! Hi, Kadan! <3\w8\1\s[10]Tegra, please stand a bit farther away; we're in public.\4\e"
		
		"\1\s[10]Tegra, how are you constantly getting underfoot? You're worse than a child.\w8\0\s[0]What? I'm literaly, like, standing here.\w8\1\s[10]\4\n\n[half]Well, don't.\e"
		
		"\1\s[10]Do you have no concept of personal space? Shoo!\w8\0\s[0]Awwwww. </3\5\e"		
	}
	elseif reference2 == 1 && nowshell == "mirror"
	{
		"\1\s[10]\i[253]Did you need something\w3, Tegra?\w8\0\n\n[half]\s[0]\i[253]\i[202]\i[406]\i[407]Oops! Sorry, ma'am!\i[152]\4\e"

		"\0\s[0]\i[210]\i[253]Ma'am?\w4\i[406] The Terran is still here.\w8\1\n\n[half]\s[10]\i[201]\i[251]Ah\w3, that's right. \w4How irritating.\4\e"
