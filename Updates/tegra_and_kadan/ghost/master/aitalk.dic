OnSurfaceRestore
 {
 	if reference0 == 1 && reference1 != 11  //This is checking what poses they are in. 
 	{
	tegrasleep = "on"
	
 	"\0\s[1]...\w8zzz\w8...\w8zzz\w8...\1\s[10]\e"
 	}
 	elseif reference1 == 11 && reference0 != 1
 	{
	kadansleep = "on"
 	"\0\s[0]\1\s[11]...\w8zzz\w8...\w8zzz\w8...\e"
 	}
	elseif reference1 == 11 && reference0 == 1
	{
	tegrasleep = "on"
	kadansleep = "on"
	
	"\1\s[11]...\w8zzz\w8...\w8zzz\w8...\0\s[1]...\w8zzz\w8...\w8zzz\w8...\e"
	}
 	else
 	{
	tegrasleep = "off"
	kadansleep = "off"
	"\0\s[0]\1\s[10]\e"
	}
 }
 
